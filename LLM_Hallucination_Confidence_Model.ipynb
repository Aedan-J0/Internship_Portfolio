    "import pandas as pd\n",
    "from sklearn.feature_extraction.text import TfidfVectorizer\n",
    "from sklearn.metrics.pairwise import cosine_similarity\n",
    "import json"
   ],
   "outputs": [],
   "execution_count": 1
  },
  {
   "cell_type": "code",
   "id": "user_history_system",
   "metadata": {},
   "source": [
    "class UserHistorySystem:\n",
    "    def __init__(self):\n",
    "        # User-specific databases\n",
    "        self.user_profiles = {}\n",
    "        # Vectorizer for queries\n",
    "        self.vectorizer = TfidfVectorizer()\n",
    "\n",
    "    def initialize_with_dataset(self, dataset_path):\n",
    "        \"\"\"Initialize the system with a preloaded dataset.\"\"\"\n",
    "        with open(dataset_path, 'r') as f:\n",
    "            data = json.load(f)\n",
    "        # Prepopulate a general dataset\n",
    "        self.user_profiles['general'] = {\n",
    "            \"queries\": [entry[\"user_query\"] for entry in data],\n",
    "            \"responses\": [entry[\"chatgpt_response\"] for entry in data],\n",
    "        }\n",
    "        # Fit the vectorizer on the general queries\n",
    "        self.user_profiles['general']['vectorized_queries'] = self.vectorizer.fit_transform(\n",
    "            self.user_profiles['general']['queries']\n",
    "        )\n",
    "\n",
    "    def find_best_response(self, user_id, query):\n",
    "        \"\"\"Find the best response for a given query.\"\"\"\n",
    "        # Check if user profile exists, otherwise use 'general'\n",
    "        profile = self.user_profiles.get(user_id, self.user_profiles['general'])\n",
    "        if not profile.get(\"vectorized_queries\"):\n",
    "            return None\n",
    "\n",
    "        # Vectorize the new query\n",
    "        query_vector = self.vectorizer.transform([query])\n",
    "        # Compute similarities\n",
    "        similarities = cosine_similarity(query_vector, profile[\"vectorized_queries\"])\n",
    "        best_match_index = similarities.argmax()\n",
    "        best_match_score = similarities[0, best_match_index]\n",
    "\n",
    "        # Check if similarity meets threshold\n",
    "        threshold = 0.8  # Similarity threshold\n",
    "        if best_match_score >= threshold:\n",
    "            return profile[\"responses\"][best_match_index]\n",
    "        return None\n",
    "\n",
    "    def add_to_profile(self, user_id, query, response):\n",
    "        \"\"\"Add a query-response pair to the user's profile.\"\"\"\n",
    "        if user_id not in self.user_profiles:\n",
    "            # Initialize profile for new user\n",
    "            self.user_profiles[user_id] = {\"queries\": [], \"responses\": [], \"vectorized_queries\": None}\n",
    "\n",
    "        # Add the new query and response\n",
    "        self.user_profiles[user_id][\"queries\"].append(query)\n",
    "        self.user_profiles[user_id][\"responses\"].append(response)\n",
    "        # Refit the vectorizer for this user\n",
    "        self.user_profiles[user_id][\"vectorized_queries\"] = self.vectorizer.fit_transform(\n",
    "            self.user_profiles[user_id][\"queries\"]\n",
    "        )\n",
    "\n",
    "    def get_response(self, user_id, query, llm_query_function):\n",
    "        \"\"\"Get a response for a user query.\"\"\"\n",
    "        # Try to find a response from the user profile\n",
    "        response = self.find_best_response(user_id, query)\n",
    "        if response is not None:\n",
    "            return response\n",
    "\n",
    "        # If no good match, query the LLM\n",
    "        response = llm_query_function(query)\n",
    "        # Add the new query-response pair to the profile\n",
    "        self.add_to_profile(user_id, query, response)\n",
    "        return response\n"
   ],
